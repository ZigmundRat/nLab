{
  "name": "nscope",
  "productName": "nScope",
  "version": "0.9.0",
  "description": "nScope",
  "private": true,
  "main": "main.js",
  "author": "David J. Meyer",
  "license": "GPL3",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "bootstrap": "^5.1",
    "cargo-cp-artifact": "^0.1",
    "concurrently": "^7.1.0",
    "css-loader": "^6.7.1",
    "electron": "^18.0",
    "electron-builder": "^24.0",
    "electron-reload": "^1.5.0",
    "got": "^11.8.5",
    "ify-loader": "^1.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "plotly.js-basic-dist": "^2.11",
    "postcss-loader": "^6.2.1",
    "sass": "^1.50",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "build-native": "cargo-cp-artifact -ac nscope-nodejs app/nscope.node  -- cargo build --message-format=json-render-diagnostics && codesign -f -s - app/nscope.node",
    "build-app":  "webpack --mode=production",
    "build": "npm run build-native && npm run build-app",
    "start": "npm run build && electron .",
    "dev": "concurrently -k \"webpack --watch --mode=development\" \"npm run build-native && electron . debug\""
  }
}